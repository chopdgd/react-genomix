{
  "name": "react-genomix",
  "version": "0.7.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "rimraf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ && yarn run build-css",
    "build-css": "cd src/stylesheets && node-sass-chokidar main.scss -o ./ --output-style compressed && mv main.css ../../dist/react-genomix.min.css"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "babel-preset-minify": "0.5.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "fixed-data-table-2": "1.0.0-beta.5",
    "immutability-helper": "^2.8.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "1.3.3",
    "papaparse": "4.6.1",
    "react": "16.5.2",
    "react-csv": "1.0.15-RC2",
    "react-dom": "16.5.2",
    "react-file-reader": "1.1.4",
    "react-scripts": "2.0.4",
    "react-virtualized": "9.20.1",
    "semantic-ui-css": "2.4.0",
    "semantic-ui-react": "0.82.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
