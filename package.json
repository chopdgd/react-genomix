{
  "name": "react-genomix",
  "version": "0.3.5",
  "description": "Core library for Nexus projects using ReactJS",
  "main": "build/index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chopdgd/react-genomix.git"
  },
  "author": "Michael A. Gonzalez <genomics.geek.04.22@gmail.com>",
  "bugs": {
    "url": "https://github.com/chopdgd/react-genomix/issues"
  },
  "homepage": "https://github.com/chopdgd/react-genomix#readme",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "3.3.10",
    "@storybook/addon-links": "3.3.10",
    "@storybook/react": "3.3.11",
    "autoprefixer": "7.2.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-app": "3.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "classnames": "2.2.5",
    "css-loader": "0.28.9",
    "dotenv": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "faker": "4.1.0",
    "file-loader": "1.1.6",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "2.30.1",
    "immutable": "3.8.2",
    "jest": "22.1.3",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "node-sass": "4.7.2",
    "object-assign": "4.1.1",
    "papaparse": "4.3.6",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.0.10",
    "promise": "8.0.1",
    "prop-types": "15.6.0",
    "react-dev-utils": "5.0.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/demo/**.{js,jsx}",
      "!src/lib/constants/**.{js,jsx}",
      "!src/lib/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/requestAnimationFrame.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/enzyme.js",
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^LibSrc(.*)$": "<rootDir>/src/lib$1",
      "^LibIndex(.*)$": "<rootDir>/src/lib/index",
      "^Stylesheets(.*)$": "<rootDir>/src/stylesheets$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "fixed-data-table-2": "0.8.8",
    "react": "16.2.0",
    "react-csv": "1.0.8",
    "react-dom": "16.2.0",
    "react-file-reader": "1.1.3",
    "react-virtualized": "9.17.3",
    "semantic-ui-css": "2.2.12",
    "semantic-ui-react": "0.77.1"
  }
}
